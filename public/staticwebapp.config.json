{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/assets/*",
        "/images/*",
        "/api/*",
        "*.{css,scss,js,png,gif,ico,jpg,jpeg,svg,woff,woff2,ttf,eot}"
      ]
    },
    "mimeTypes": {
      ".json": "application/json"
    },
    "globalHeaders": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  }